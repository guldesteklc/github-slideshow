using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp34
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] sayilar = { 1, 6, 98, 75, 40, 27, 10, 588, 54, 31, 2987 };
            foreach (int i in sayilar)
            {
                Console.WriteLine(i);
            }
            Console.Read();

        }
    }
}
