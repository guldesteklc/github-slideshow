using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp22
{
    class Program
    {
        static void Main(string[] args)
        {
            int i;
            int toplam = 0;
            
            for (i = 0; i <= 10; i += 2)
            {
                toplam = toplam + i;
            }
            Console.WriteLine("Toplam= " + toplam);
            Console.Read();

        }
    }
}
