using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp35
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] sayilar = { 2, 4, 9, 7, 3, 5 };
            int toplam = 0;
            foreach (int i in sayilar)
            {
                toplam = toplam + i;
            }
            Console.WriteLine("Toplam: " + toplam);
            Console.Read();

        }
    }
}
